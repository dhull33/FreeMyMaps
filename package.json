    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-airbnb": "^5.0.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.0",
    "ejs-lint": "^1.1.0",
    "ejs-webpack-plugin": "^1.0.0",
    "eslint": "^7.6.0",
