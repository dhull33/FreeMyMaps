  "repository": {
    "type": "git",
    "url": "https://github.com/dhull33/FreeMyMaps"
  },
  "scripts": {
    "clean": "rimraf ./public/javascripts/maps/build",
    "start": "pm2-runtime start ecosystem.config.js --env production",
