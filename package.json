    "webpack-dev-middleware": "^3.7.2"
  },
  "engines": {
    "node": "12.18.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md, ejs}": [
      "prettier --write"
    ]
  },
  "babel": {
    "presets": [
